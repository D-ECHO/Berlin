<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

 <path>FlagpoleFG.ac</path>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>3000</max-m>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Fahne</object-name>
  <property>/environment/wind-from-heading-deg</property>
  <offset-deg>-180</offset-deg>
  <factor>-1</factor>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
 </animation>

</PropertyList>
